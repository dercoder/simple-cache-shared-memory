{
    "name": "dercoder/simple-cache-shared-memory",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "psr-16",
        "simple-cache",
        "cache",
        "shared-memory",
        "shm"
    ],
    "autoload": {
        "psr-4": {
            "DerCoder\\SimpleCache\\SharedMemory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DerCoder\\SimpleCache\\SharedMemory\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alexander Fedra",
            "email": "alexander.fedra@example.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-sysvshm": "*",
        "ext-zlib": "*",
        "psr/simple-cache": "^1.0",
        "symfony/polyfill-php82": "^1.30"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12 || ^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0"
    },
    "suggest": {
        "ext-igbinary": "For faster serialization of values"
    }
}
